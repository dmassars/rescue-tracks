{
  "name": "rescue_tracks",
  "version": "0.0.1",
  "description": "An API for a Rescue Adoption Event tracker",
  "author": "Tim Stephens <timothy.f.stephens@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon",
    "prestart:prod": "tsc",
    "start:prod": "node dist/server.js",
    "migrate": "tsc && MIGRATE=true typeorm migration:run",
    "migrate:rollback": "tsc && MIGRATE=true typeorm migration:revert",
    "migration:generate": "tsc && MIGRATE=true typeorm migration:generate",
    "migration:create": "tsc && MIGRATE=true typeorm migration:create"
  },
  "dependencies": {
    "@nestjs/common": "^4.6.6",
    "@nestjs/core": "^4.6.6",
    "@nestjs/microservices": "^4.6.6",
    "@nestjs/testing": "^4.6.6",
    "@nestjs/typeorm": "^2.0.0",
    "@nestjs/websockets": "^4.6.6",
    "@types/express": "^4.11.1",
    "@types/express-jwt": "0.0.38",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/node": "^9.4.0",
    "@types/socket.io": "^1.4.33",
    "@types/superagent": "^3.5.8",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "pg": "^7.4.2",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.10",
    "socket.io": "^2.1.0",
    "superagent": "^3.8.3",
    "typeorm": "^0.2.7",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "nodemon": "^1.17.3",
    "ts-node": "^6.0.2"
  }
}
